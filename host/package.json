{
    "name": "stanleybet",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start-ro": "ng serve -c=ro",
        "build": "ng build",
        "build:stb-stage": "ng build --configuration stb-stage",
        "build:95-stb": "ng build --configuration 95-stb",
        "build:rc-stb": "ng build --configuration rc-stb && ng run stanleybet:server:rc-stb && cpx proxy-server.js dist/stanleybet",
        "build:stage-stb": "ng build --configuration stage-stb",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "dev:ssr": "ng run stanleybet:serve-ssr",
        "prerender": "ng run stanleybet:prerender",
        "serve:ssr": "node dist/stanleybet/proxy-server.js",
        "build:ssr": "ng build && ng run stanleybet:server:production && cpx proxy-server.js dist/stanleybet",
        "build:ssr-staging": "ng build --configuration stage-stb && ng run stanleybet:server:stage-stb && cpx proxy-server.js dist/stanleybet"
    },
    "private": true,
